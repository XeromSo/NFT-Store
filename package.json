{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "dev": "npm-run-all -p dev:*",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "sass": "sass --watch app/:app/",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:worker": "esbuild ./app/entry.worker.js --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev:worker": "esbuild ./app/entry.worker.js --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.7",
    "@remix-run/react": "^1.6.7",
    "@remix-run/serve": "^1.6.7",
    "@tippyjs/react": "^4.2.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "framer-motion": "^7.3.5",
    "gsap": "^3.11.1",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "i18next-fs-backend": "^1.1.5",
    "i18next-http-backend": "^1.4.1",
    "lottie-react": "^2.4.0",
    "nanoid": "^4.0.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nprogress": "^0.2.0",
    "rc-slider": "^10.0.1",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-highlight-words": "^0.18.0",
    "react-i18next": "^11.18.4",
    "react-icons": "^4.4.0",
    "react-id-generator": "^3.0.2",
    "react-select": "^5.4.0",
    "react-table": "^7.7.0",
    "remix": "^1.7.1",
    "remix-i18next": "^4.1.1",
    "remix-pwa": "^1.0.1",
    "sass": "^1.54.0",
    "swiper": "^8.3.2",
    "usehooks-ts": "^2.6.0",
    "web-push": "^3.4.5",
    "web3": "^1.8.0",
    "zustand": "^4.0.0-rc.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/node-persist": "^3.1.2",
    "eslint": "^8.20.0"
  },
  "engines": {
    "node": ">=14"
  }
}
